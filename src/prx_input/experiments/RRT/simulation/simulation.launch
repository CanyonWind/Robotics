<launch>

  <!-- Load independent files -->
        
  <rosparam ns="" command="load" file="$(find prx_input)/templates/applications/empty_application.yaml"/>
  <rosparam ns="" command="load" file="$(find prx_input)/templates/simulators/collision_stop_simulator.yaml"/>
  <rosparam ns="" command="load" file="$(find prx_input)/templates/spaces/space_types.yaml"/>
  <rosparam command="load" file="$(find prx_input)/templates/spaces/space_types.yaml"/>

  <!-- Load template files -->
        
  <rosparam command="load" ns="controller" file="$(find prx_input)/templates/controllers/consumer.yaml"/>
  <rosparam command="load" ns="plant" file="$(find prx_input)/templates/plants/second_order_car.yaml"/>
  
  <!-- Override template parameters -->
        
  <rosparam ns="controller">
  </rosparam>
        
  <rosparam ns="plant">
    initial_state: [0,0,0,0,0]
  </rosparam>
       
  <!-- Set up concrete system tree -->
   
  <rosparam command="load" ns="simulator/subsystems/consumer">
    template: "controller"
  </rosparam>
        
  <rosparam command="load" ns="simulator/subsystems/consumer/subsystems/car">
    template: "plant"
    state_space: 
      min: [-400, -400, -4, 0, -0.1]
      max: [400, 400, 4, 50, 0.1]
    input_control_space: 
      min: [-10, -.04]
      max: [ 10,  .04]
        
  </rosparam>
  
  <!-- Define the simulation node -->

</launch>
